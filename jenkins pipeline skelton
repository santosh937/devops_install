pipeline {
    agent any 

     stages {
        stage('CHECKOUT') {
            steps {
                echo "This is Checkout stage"
            }
        }

        stage('BUILD') {
            steps {
                echo "This is Build stage"
            }
        }

        stage('TEST') {
            steps {
                echo "This is Test stage"
            }
        }

        stage('DEPLOY') {
            steps {
                echo "This is Deploy stage"
            }
        }
    }
}
